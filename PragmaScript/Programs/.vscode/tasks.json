{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "..\\bin\\x64\\Release\\pragma.exe",
    "isShellCommand": true,
    "args": ["-O0", "${file}", "-run", "-D"],
    "showOutput": "always",
    "problemMatcher": {
        // "owner": "ps",
        "fileLocation":  "absolute",
        "pattern":{
            "regexp": "error: (.*?) at \\(.*?file \"(.*?)\".*?line (.*?),.*?pos (.*?),",
            "message": 1,
            "file": 2,
            "line": 3,
            "column": 4
        }

    }
    
}