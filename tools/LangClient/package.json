{
	"name": "pragma-language-server",
	"displayName": "pragma-language-server",
	"description": "things",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:pragma"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "pragma",
				"aliases": [
					"Pragma",
					"pragma"
				],
				"extensions": [
					".prag"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pragma",
				"scopeName": "source.pragma",
				"path": "./syntaxes/pragma.tmLanguage.json"
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.0.5",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^7.2.0",
		"tslint": "^6.1.2",
		"typescript": "^3.3.1",
		"vscode-languageclient": "^6.1.3",
		"vscode-languageserver": "^6.1.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {}
}
